#include <iostream>
using namespace std;

class construct
{

public:
    float area;

    construct()
    {
        area = 0;
    }

    construct(int a, int b)
    {
        area = a * b;
    }
    construct(int a)
    {
        int b =6;
    area = a * b;
    }

    void disp()
    {
        cout << area << endl;
    }
};

int main()
{

    construct o;
    construct o2(10, 20);
    construct o3(2);

    o.disp();
    o2.disp();
    o3.disp();
    return 1;
}
